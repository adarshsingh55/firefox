{
  "manifest_version": 2,
  "name": "My Extension",
  "version": "1.0",
  "description": "My Firefox Extension",
  "background": {
    "persistent": true
  },
  "browser_action": {
    "default_title": "My Extension"
  },
  "permissions": ["tabs"],
  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}
